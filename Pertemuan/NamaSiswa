package Praktikum5;

public class Siswa {
    private String Nim;
    private String Nama;
    private int NilaiTeori;
    private int NilaiPraktek;

// getter and setter methods
    public String getNim() {return Nim;}
    public void setNim(String Nim) {this.Nim = Nim;}
    public String getNama() {return Nama;}
    public void setNama(String Nama) {this.Nama = Nama;}
    public int getNilaiTeori() {return NilaiTeori;}
    public void setNilaiTeori(int NilaiTeori) {this.NilaiTeori = NilaiTeori;}
    public int getNilaiPraktek() {return NilaiPraktek;}
    public void setNilaiPraktek(int NilaiPraktek) {this.NilaiPraktek = NilaiPraktek;}

//    default constructor
    public Siswa(){
        super();
    }

//    parameterized constructor
    public Siswa(String Nim, String Nama, int NilaiTeori, int NilaiPraktek){
        super();
        this.Nim = Nim;
        this.Nama = Nama;
        this.NilaiTeori = NilaiTeori;
        this.NilaiPraktek = NilaiPraktek;
    }
}
